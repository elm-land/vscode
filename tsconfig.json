{
	"compilerOptions": {
		"module": "commonjs",
		"target": "es2020",
		"lib": ["es2020"],
		"outDir": "dist",
		"sourceMap": true,
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
